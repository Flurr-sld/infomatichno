x = int(input())
for i in range(2, x):
    is_prime = True

    for i in range(2, x):
        if x % i == 0:
            is_prime = False

    if x == 1:
        print("1 - не является ни простым ни составным числом")
    else:
        if is_prime:
            print("X - простое число")
        else:
            print("X - не является простым числом")
